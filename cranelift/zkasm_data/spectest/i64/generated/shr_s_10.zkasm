start:
  0xffff => SP
  zkPC + 2 => RR
  :JMP(function_1)
  :JMP(finalizeExecution)
function_1:
  RR :MSTORE(SP)
  SP - 1 => SP
  B :MSTORE(SP - 1)
  SP - 2 => SP
  1n => A  ;; LoadConst64
  9223372036854775807n => B  ;; LoadConst64
  $ => A :sra
  0n => B  ;; LoadConst64
  B :ASSERT
  SP + 2 => SP
  $ => B :MLOAD(SP - 1)
  SP + 1 => SP
  $ => RR :MLOAD(SP)
  :JMP(RR)
finalizeExecution:
  ${beforeLast()}  :JMPN(finalizeExecution)
                   :JMP(start)
INCLUDE "helpers/2-exp.zkasm"