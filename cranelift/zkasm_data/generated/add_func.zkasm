start:
  zkPC + 2 => RR
  :JMP(function_1)
  :JMP(finalizeExecution)
function_1:
  SP + 2 => SP
  RR :MSTORE(SP)
  2 => A
  3 => B
  zkPC + 2 => RR
  :JMP(function_2)
  5 => B
  B :ASSERT
  $ => RR :MLOAD(SP)
  SP - 2 => SP
  :JMP(RR)
function_2:
  $ => A :ADD
  :JMP(RR)
finalizeExecution:
  ${beforeLast()}  :JMPN(finalizeExecution)
                   :JMP(start)