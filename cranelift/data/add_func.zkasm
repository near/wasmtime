start:
  zkPC + 2 => RR
  :JMP(function_1)
  :JMP(finalizeExecution)

function_1:
  SP + 1 => SP
  RR :MSTORE(SP)
  SP + 1 => SP
L0:
  CTX :MSTORE(SP)
  0 + 2 => A
  0 + 3 => B
  $ => CTX :MLOAD(SP)
  JMP(function_2)
  0 + 5 => B
  $ => CTX :MLOAD(SP)
  B :ASSERT
  :JMP(L1)
L1:
  SP - 1 => SP
  $ => RR :MLOAD(SP)
  SP - 1 => SP
  :JMP(RR)


function_2:
L0:
  $ => A :ADD
  :JMP(L1)
L1:
  :JMP(RR)

finalizeExecution:
  ${beforeLast()}  :JMPN(finalizeExecution)
                   :JMP(start)

