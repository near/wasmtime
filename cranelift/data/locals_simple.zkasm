start:
  zkPC + 2 => RR
  :JMP(function_1)
  :JMP(finalizeExecution)

function_1:
  SP + 1 => SP
  RR :MSTORE(SP)
L1_0:
  0 + 2 => A
  0 + 2 => B
  B :ASSERT
  :JMP(L1_1)
L1_1:
  $ => RR :MLOAD(SP)
  SP - 1 => SP
  :JMP(RR)
finalizeExecution:
  ${beforeLast()}  :JMPN(finalizeExecution)
                   :JMP(start)

