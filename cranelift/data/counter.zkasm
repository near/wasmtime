start:
  zkPC + 2 => RR
  :JMP(function_0)
  :JMP(finalizeExecution)
function_0:
  SP + 1 => SP
  RR :MSTORE(SP)
L0:
  0 + 0 => A
  :JMP(L1)
L1:
  0 + 1 => B
  $ => A :ADD
  0 + 10 => B
  $ => B :EQ
  B :JMPNZ(L3)
  :JMP(L2)
L2:
  :JMP(L1)
L3:
  0 + 10 => B
  B :ASSERT
  :JMP(L4)
L4:
  $ => RR :MLOAD(SP)
  SP - 1 => SP
  :JMP(RR)

finalizeExecution:
  ${beforeLast()}  :JMPN(finalizeExecution)
                   :JMP(start)

